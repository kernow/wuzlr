<h2>Edit Account</h2>

<% semantic_form_for @user do |form| %>
  <%= form.error_messages %>
  <%= render :partial => "/users/inputs", :locals => { :form => form } %>
  <% form.buttons do %>
    <%= form.commit_button "Save" %>
  <% end %>
<% end %>

